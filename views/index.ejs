<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900" rel="stylesheet" type="text/css">

    <title> GENOME LINK REPORTS</title>

    
      <link href="https://genomelink.io/static/img/favicon.85bf6e763fdb.png" rel="icon" type="image/png">

      <link href="https://genomelink.io/static/font-awesome/css/font-awesome.min.bf0c425cdb73.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/devicon/devicon.min.751efaf7388a.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/animate.d6f644ffbb7b.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/bootstrap.min.5c7070ef655a.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/plugins/select2/select2.min.138258aaab4e.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/highlight/tomorrow-night.d372a77090f6.css" rel="stylesheet" >

      <link href="https://genomelink.io/static/css/developers/pe-icons/pe-icon-7-stroke.db205f364aaf.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/developers/pe-icons/helper.b041b560d4bd.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/developers/stroke-icons/style.a4210ee18f11.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/developers/style.a810a51d72c5.css" rel="stylesheet">
      <link href="https://genomelink.io/static/css/developers/genomelink.f8d5ac355f35.css" rel="stylesheet">
    

    
	  <link href="https://genomelink.io/static/css/plugins/dataTables/datatables.min.a0732f4732fb.css" rel="stylesheet">
  </head>
  <body>

        
    <script>
      localStorage.removeItem("loginRedirectURL");
    </script>

    <div class="wrapper">

      
      <section class="content">
        
<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12">
      <div class="view-header">
        <div class="header-icon">
          <i class="pe page-header-icon pe-7s-server"></i>
        </div>
        <div class="header-title">
          <h1>Genome Link Vitamin Reports</h1>         
          <% if (authorize_url) { %>
            <a href="<%= authorize_url %>" class="btn btn-default">Connect with genome</a>
          <% } %>

        </div>
      </div>
      <hr>
    </div>
  </div>

  
        <% if (reports.length > 0) { 

            for (let report of reports) { 
              if (allreports[report.phenotype.url_name] != undefined) 
                allreports[report.phenotype.url_name]['summary'] = report.summary; 
            }
    
        %>    

        <div class="row">
          <div class="table-responsive">
            <table id="listOfPhenotyesTable" class="table table-striped table-bordered">
              <thead>
                <tr>
                  <th></th>
                  <th>Name</th>
                  <th>Scores</th>
                  <th>Description</th>
                </tr>
              </thead>
              <tbody>
      
      <% for (let key in allreports) { %>
        <tr>
          <td><img src="https://genomelink.io/static/img/developers/phenotypes/<%= key %>.png"></td>
          <td>
            <strong><%= allreports[key]['name'] %></strong><br>
            <code><%= key %></code>
            <h1> <%= allreports[key]['summary'].score %>           
            </h1>
          </td>
          <td width="25%">
            <ul class="list-unstyled">
              <% for (let scorekey in allreports[key]['scores']) { %>
                <li><code><%= scorekey %></code> = <%= allreports[key]['scores'][scorekey] %></li>
              <% } %>
            </ul>
          </td>
          <td width="55%"><%= allreports[key]['description'] %>
          </td>
        </tr>
      <% } %>
                    
                    
              </tbody>
            </table>
          </div>
    
        </div>

        <% } %>
        </div>

      </section>
    </div>


    <script src="https://genomelink.io/static/js/jquery-3.1.1.min.e071abda8fe6.js"></script>
    <script src="https://genomelink.io/static/js/bootstrap.min.5869c96cc8f1.js"></script>

    <script src="https://genomelink.io/static/js/plugins/pace/pace.min.24d2d5e3e331.js"></script>
    <script src="https://genomelink.io/static/js/plugins/jasny/jasny-bootstrap.min.bfff7e5b435e.js"></script>
    <script src="https://genomelink.io/static/js/plugins/chosen/chosen.jquery.48e92f631c7c.js"></script>
    <script src="https://genomelink.io/static/js/plugins/select2/select2.full.min.788f1c2f54ad.js"></script>
    <script src="https://genomelink.io/static/js/highlight.pack.19a95a9e6272.js"></script>
    <script>hljs.initHighlightingOnLoad();</script>

    <script src="https://genomelink.io/static/js/developers/developers.6b804ac08283.js"></script>

    <script>
      $(document).ready(function(){
        $("select").addClass("select2");
        $(".select2").select2({
          minimumResultsForSearch: Infinity,
        });
      })
    </script>

    
<script src="https://genomelink.io/static/js/plugins/dataTables/datatables.min.d70f267acd2b.js"></script>
<script>
$(document).ready(function() {
  $('#listOfPhenotyesTable').DataTable({
    paging: false,
    order: [[ 1, "asc" ]],
    columnDefs: [
      { targets: [1], orderable: true },
      { targets: '_all', orderable: false }
    ]
  });
});
</script>


  </body>
</html>
